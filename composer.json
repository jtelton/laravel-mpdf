{
    "name": "praem90/laravel-mpdf",
    "description": "A mPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "mpdf", "pdf"],
    "authors": [
        {
            "name": "Mohan Raj M",
            "email": "praem1990@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/support": "^5.5",
        "mpdf/mpdf": "^7.0.0"

    },

    "autoload": {
        "psr-4": {
            "Praem90\\PDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Praem90\\PDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Praem90\\PDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}